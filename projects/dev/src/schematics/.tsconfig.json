{
  "compilerOptions": {
    "lib": ["es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "strictNullChecks": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "target": "es6",
    "types": [
      "jasmine",
      "node"
    ],
    "baseUrl": "./"
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}
