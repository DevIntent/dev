{
  "name": "dev-demo",
  "description": "DevIntent Application Development Tools",
  "version": "0.3.0",
  "repository": "https://github.com/DevIntent/dev",
  "homepage": "https://devintent-dev.web.app",
  "bugs": "https://github.com/DevIntent/dev/issues",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve dev-demo",
    "build": "ng build dev-demo",
    "build:prod": "ng build dev-demo --prod",
    "build:lib": "ng build dev && npm run build:lib:scss",
    "build:lib:scss": "scss-bundle -c scss-bundle.config.json",
    "build:schematics": "tsc -p ./projects/schematics/tsconfig.json",
    "publish:lib": "npm run lint && npm run build:lib && npm publish ./dist/dev --access public",
    "publish:schematics": "npm run build:schematics && npm publish ./projects/schematics --access public",
    "deploy": "ng deploy",
    "test": "ng test && npm run test:schematics",
    "test:schematics": "npm run build:schematics && jasmine ./projects/schematics/src/**/*_spec.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "smex": "source-map-explorer",
    "update": "npm-check -u"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "~8.1.4",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "~8.0.0-beta.27",
    "@angular/forms": "~8.2.5",
    "@angular/material": "~8.1.4",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "core-js": "~2.6.9",
    "firebase": "~6.6.0",
    "hammerjs": "~2.0.8",
    "logrocket": "^1.0.3",
    "rxjs": "~6.5.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular-devkit/core": "^8.3.3",
    "@angular-devkit/schematics": "^8.3.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.4",
    "codelyzer": "^5.1.0",
    "firebase-tools": "^6.10.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.5.0",
    "npm-check": "~5.9.0",
    "protractor": "~5.4.2",
    "rxjs-tslint": "^0.1.7",
    "scss-bundle": "^2.5.1",
    "source-map-explorer": "^2.0.1",
    "ts-node": "^8.3.0",
    "tsickle": "^0.37.0",
    "tslib": "~1.10.0",
    "tslint": "^5.20.0",
    "typescript": "~3.5.3"
  }
}
