{
  "hosting": [
    {
      "target": "dev-demo",
      "public": "dist/dev-demo",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "appAssociation": "AUTO",
      "rewrites": [
        {
          "source": "/d/**",
          "dynamicLinks": true
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
